<%=form_for @user do |f| %>
<%=f.label :name%>
<%=f.text_field :name%><br>
<%=f.label :age%>
<%=f.number_field :age%><br>
<%=f.label :email%>
<%=f.email_field :email%><br>
<%=f.label :phone%>
<%=f.telephone_field :phone%><br>
<%=f.label :password%>
<%=f.password_field :password%><br>
<%=f.submit%>
<%end%>
